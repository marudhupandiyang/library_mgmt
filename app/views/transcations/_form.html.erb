<% unless @transcation.errors.empty? %>
	<div id="error"> <%= @transcation.errors.full_messages %> </div> <br/>
<% end %>



<%= form_for (@transcation) , :remote => true  do |f| %>

	<%= f.label :book_id %>
	<%= f.text_field :book_id %>

	<br/><br/>

	<%= label_tag :regno %>
	<%= text_field_tag :regno %>

	<br/>
	<%= f.submit %>


<%= link_to "Back to transcation" , transcations_path %>

<% end %>
